{
  "project-settings": {
    "code-owner": "React App",
    "bundle-path": "../core/src/main/java",
    "test-path": "../core/src/test/java",
    "apps-path": "../ui.apps/src/main/content/jcr_root/apps",
    "component-path": "reactapp",
    "model-interface-pkg": "com.reactapp.core.models",
    "model-impl-pkg": "com.reactapp.core.models.impl"
  },
  "name": "error-page",
  "title": "Página de Erro",
  "group": "React App - Content",
  "type": "components",
  "options": {
    "js": false,
    "jstxt": false,
    "css": false,
    "csstxt": false,
    "html": false,
    "html-content": false,
    "slingmodel": true,
    "testclass": false,
    "junit-major-version": 5,
    "content-exporter": true,
    "model-adaptables": ["request"],
    "generic-javadoc": false,
    "properties-tabs": [
      {
        "id": "tab-0",
        "label": "Textos",
        "fields": ["errorTitle", "errorParagraph"]
      },
      {
        "id": "tab-1",
        "label": "Cor do Botão",
        "fields": ["errorButtonColor"]
      }
    ],
    "properties": [
      {
        "field": "errorTitle",
        "label": "Digite um número para o erro:",
        "type": "numberfield",
        "json-expose": true,
        "json-property": "errorTitle"
      },
      {
        "field": "errorParagraph",
        "label": "Digite um texto para o parágrafo:",
        "type": "textfield",
        "json-expose": true,
        "json-property": "errorParagraph"
      },
      {
        "field": "errorButtonColor",
        "label": "Escolha uma cor para o botão:",
        "type": "textfield",
        "json-expose": true,
        "json-property": "errorButtonColor"
      }
    ]
  }
}
